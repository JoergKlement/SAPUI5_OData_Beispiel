<mvc:View 
	controllerName="sapui5.demo.odata.controller.start" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m">
	<Page 
		id="page" 
		navButtonPress="onNavPress" 
		showNavButton="true" 
		title="{i18n>startViewTitle}">
		<content>
			<GenericTile 
				id="createTile"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
				header="{i18n>createTileHeader}"
				subheader="{i18n>createTileSubheader}" 
				press="onShowData">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://add-document"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		 
			<GenericTile 
				id="readTile"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
				header="{i18n>readTileHeader}"
				subheader="{i18n>readTileSubheader}" 
				press="onShowData">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://inspection"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		</content>
	</Page>
</mvc:View>